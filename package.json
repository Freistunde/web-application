{
  "name": "web-application",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn dev-frontend\" \"yarn dev-backend\"",
    "build": "yarn dev-frontend",
    "dev-frontend": "cd frontend && cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build-frontend": "cd frontend && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dev-backend": "cd backend && nodemon server.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "bootstrap-vue": "^2.0.0-rc.9",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "lodash": "^4.17.10",
    "mapbox-gl": "^0.45.0",
    "mapbox-gl-vue": "^1.9.0",
    "morgan": "~1.9.0",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "jquery": "^3.3.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.17.5",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
